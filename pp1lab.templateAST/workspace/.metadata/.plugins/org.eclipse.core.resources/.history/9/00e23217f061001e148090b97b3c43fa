            buffer.append(tab+"  null");
        buffer.append("\n");

        if(const_to_print!=null)
            buffer.append(const_to_print.toString("  "+tab));
        else
            buffer.append(tab+"  null");
        buffer.append("\n");

        buffer.append(tab);
        buffer.append(") [Print]");
